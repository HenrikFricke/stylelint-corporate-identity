{
  "name": "stylelint-corporate-identity",
  "scripts": {
    "test": "yarn test:unit && yarn test:rule",
    "test:unit": "jest --coverage",
    "test:rule": "node src/__tests__/index.js"
  },
  "main": "src/index.js",
  "jest": {
    "testRegex": "/__tests__/.*\\.test\\.js$",
    "testEnvironment": "node"
  },
  "author": "Henrik Fricke <henrikfricke@web.de>",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.4",
    "stylelint": "^7.10.1"
  },
  "devDependencies": {
    "jasmine-data-provider": "^2.2.0",
    "jest": "^19.0.2",
    "stylelint-test-rule-tape": "^0.2.0"
  },
  "engines": {
    "node": ">=6.11.0"
  }
}
